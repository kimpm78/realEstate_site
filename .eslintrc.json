{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings"],
  "plugins": ["import"],
  "globals": {
    "module": "readonly"
  },
  "rules": {
    "no-irregular-whitespace": "off"
  }
}
